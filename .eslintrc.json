{
  "extends": [
    "plugin:vue/recommended",
    "eslint:recommended"
  ],
  "rules": {
    "no-console": "warn",
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "comma-dangle": [
      "warn",
      "always-multiline"
    ]
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}